package com.selenium4.selenium4jar;

    


import static org.openqa.selenium.support.locators.RelativeLocator.withTagName;

import java.util.concurrent.TimeUnit;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;




/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        System.out.println( "Hello World!" );
        
        System.out.println( "Hello World!" );
        
        System.setProperty("webdriver.chrome.driver", "D:\\software\\browser\\chromedriver_win32\\chromedriver.exe");
        		
        
        	WebDriver driver = new ChromeDriver();
        	
        	System.out.println(TimeUnit.SECONDS);
        	
        
        	driver.findElement(withTagName("test"));
        	
        	//driver.findElement(By.tagName("xxx").b);
        
        		    }

	
	}

