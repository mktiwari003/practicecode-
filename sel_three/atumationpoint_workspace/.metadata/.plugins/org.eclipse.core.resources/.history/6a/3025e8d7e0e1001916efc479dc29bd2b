	// int a=40;//non static  
	
	void m(){
		
		System.out.println("testing file");
	} 
	
	static{System.out.println("static block is invoked");}  
	
	
  public static void main(String args[]){ 
	  System.out.println(a);
	   System.out.println("Hello main");  
	  } 
